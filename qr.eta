<!DOCTYPE html>
<html>
<head>
    <style>
        table, th, td {
            border: 1px solid black;
            border-collapse: collapse;
        }

        td {
            width: 5cm;
            text-align: center;
        }

        img {
            width: 100%;
        }
    </style>
</head>
<body>
<table>
    <% it.imgLinks.forEach(function(chunk) { %>
    <tr>
        <% chunk.forEach(function(qr) { %>
        <td>
            <img src="<%= qr %>" />
            <div>http://voteabroad.info</div>
        </td>
        <% }) %>
    </tr>
    <% }) %>
</table>
</body>
</html>